{
  "name": "@remoteoss/remote-json-schema-form-kit",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.15.2",
  "description": "Remote's kit for json-schema-form",
  "author": "Remote.com <engineering@remote.com> (https://remote.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/remoteoss/remote-json-schema-form-kit.git"
  },
  "keywords": [
    "JSON",
    "schema",
    "json-schema",
    "form"
  ],
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "files": [
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=18.14.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "NODE_ENV=development tsup --watch",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:file": "jest --runTestsByPath --watch",
    "lint": "eslint --max-warnings 0 .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "check": "pnpm run lint && pnpm run typecheck",
    "prepare": "husky && pnpm run build"
  },
  "dependencies": {
    "@remoteoss/json-schema-form": "1.2.6",
    "@remoteoss/json-schema-form-v0-deprecated": "npm:@remoteoss/json-schema-form@0.12.2-beta.0",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.14.0",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-typescript": "^7.26.0",
    "@jest/globals": "^29.7.0",
    "@jest/test-sequencer": "^29.7.0",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "eslint": "^9.18.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  }
}
